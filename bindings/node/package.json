{
  "name": "@elkyn/waldb",
  "version": "0.1.0",
  "description": "Node.js bindings for WalDB - High-performance write-ahead log database with tree semantics",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "index.node",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "cargo-cp-artifact -ac waldb-node index.node -- cargo build --message-format=json-render-diagnostics",
    "build-debug": "npm run build --",
    "build-release": "npm run build -- --release",
    "test": "npm run test-fundamentals && npm run test-types && npm run test-complete",
    "test-fundamentals": "node test-fundamentals.js",
    "test-types": "node test-types.js",
    "test-complete": "node test-complete.js"
  },
  "keywords": [
    "database",
    "tree",
    "lsm",
    "firebase",
    "realtime",
    "waldb",
    "native"
  ],
  "author": "Elkyn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/elkyn/waldb"
  },
  "devDependencies": {
    "cargo-cp-artifact": "^0.1.8"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "mongodb": "^6.18.0"
  }
}
